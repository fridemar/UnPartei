[[code format="clojure"]]
(fn [request response]
  (do
    (.setCharacterEncoding response "UTF-8")
    (hiccup.core/html "<!DOCTYPE html>"
      [:html
        [:head
          [:meta {:http-equiv "Content-Type" :content "text/html;charset=utf-8"}]
          [:title "HelloWorld"]]
        [:body
          [:h1 "HelloWorld"]]])))

[[code]]

